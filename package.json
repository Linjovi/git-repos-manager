{
  "name": "git-repos-manager",
  "version": "1.0.4",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon src/main.ts",
    "build:clean": "rm -rf .next",
    "build:server": "npx tsc --project tsconfig.server.json && npx babel .next/production-server -d .next/production-server --config-file ./.babelrc-server --extensions \".js\"",
    "build:ui": "npx next build",
    "build": "npm run build:clean && npm run build:server && npm run build:ui",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "NODE_ENV=production node --require reflect-metadata .next/production-server/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "bin": {
    "grm": "bin/index.sh"
  },
  "dependencies": {
    "@babel/core": "^7.1.0",
    "@nestjs/common": "^6.0.0",
    "@nestjs/core": "^6.0.0",
    "@nestjs/platform-express": "^6.0.0",
    "@nestjs/platform-socket.io": "^6.5.3",
    "@nestjs/platform-ws": "^6.5.3",
    "@nestjs/websockets": "^6.5.3",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-typescript": "^1.1.1",
    "antd": "^3.20.7",
    "ini": "^1.3.5",
    "mrgx": "^1.6.0",
    "nest-next": "^0.7.0",
    "nest-next-module": "^0.2.4",
    "next": "^8.0.0",
    "next-compose-plugins": "^2.2.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.0",
    "redux": "^4.0.4",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^2.6.2",
    "rxjs": "^6.3.3",
    "shelljs": "^0.8.3",
    "socket.io-client": "^2.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-decorators": "^7.1.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-typescript": "^7.1.0",
    "@nestjs/testing": "6.1.1",
    "@types/express": "4.16.1",
    "@types/ini": "^1.3.30",
    "@types/jest": "24.0.11",
    "@types/next": "^8.0.6",
    "@types/node": "11.13.4",
    "@types/qs": "^6.5.3",
    "@types/react": "^16.8.24",
    "@types/react-dom": "^16.8.5",
    "@types/react-redux": "^7.1.1",
    "@types/redux": "^3.6.0",
    "@types/shelljs": "^0.8.5",
    "@types/socket.io": "^2.1.2",
    "@types/socket.io-client": "^1.4.32",
    "@types/supertest": "2.0.7",
    "babel-plugin-module-resolver": "^3.1.1",
    "jest": "24.7.1",
    "nodemon": "^1.19.1",
    "prettier": "1.17.0",
    "supertest": "4.0.2",
    "ts-jest": "24.0.2",
    "ts-node": "8.1.0",
    "tsc-watch": "2.2.1",
    "tsconfig-paths": "3.8.0",
    "tslint": "5.16.0",
    "typescript": "3.4.3"
  },
  "files": [
    "/.next",
    "/bin"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
